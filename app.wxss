.u-btn-default:active{background-color: #fa5c70;}
.u-btn-disabled{background-color: #aaaaaa;color:#fff;margin: 20rpx 40rpx;border-radius: 8rpx;}
.u-link-btn{display: inline-block;padding:10rpx;border:1px solid #DB384C;font-size: 28rpx;color:#DB384C;border-radius: 4rpx;}
form button{background-color: #fff;}
/*buttom end*/
/*单选列表样式 start*/
.m-check-label{-webkit-tap-highlight-color: rgba(0, 0, 0, 0);}
.m-check{position: absolute;left: -9999em;}
.m-icon-checked{display: inline-block;vertical-align: middle;font: normal normal normal 14px/1 "weui";font-size: inherit;text-rendering: auto;-webkit-font-smoothing: antialiased;}
.m-check-label {-webkit-tap-highlight-color: rgba(0, 0, 0, 0);}
.m-check-label:active {background-color: #ECECEC;}
.m-cells-checkbox .m-cell-hd {padding-right: 12rpx;}
/*单选列表样式 end*/
/*列表布局 start*/
