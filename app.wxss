/*页面重置样式 start*/
page {padding-bottom:1rpx; background-color: #282B30;font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;}
/*页面重置样式 start*/
/*buttom start*/
.u-btn{position: relative;display: block;margin-left: auto;margin-right: auto;padding:0 28rpx;box-sizing: border-box;font-size: 16px;text-align: center;line-height: 2.55555556;}
